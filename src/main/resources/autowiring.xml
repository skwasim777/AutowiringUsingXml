<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="address" class="com.java.Address">
		<property name="street" value="idgahMohalla"></property>
		<property name="city" value="Bhatwadgaon"></property>
		<property name="state" value="maharashtra"></property>
		<property name="country" value="india"></property>
	</bean>
	<bean id="emp" class="com.java.Employee" autowire="byType">
		<property name="eId" value="101"></property>
		<property name="eName" value="Wasim Shaikh"></property>
		<property name="eEmail" value="wasimsk6061@gmail.com"></property>

	</bean>
</beans>